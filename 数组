//逐级输出字符串因素
#include<stdio.h>
#include<stdlib.h>
int main()
{
    char arr[]="abcdef";
    int i=0;
    for(i=0;i<6;i++)
    {
        printf("%c ",arr[i]);
    }
    system("pause");
    return 0;
}

#include<stdio.h>
#include<stdlib.h>
int main()
{
    int arr[3][4]={1,2,3,4,5,6,7,8,9,0,11,12};
    int i=0;
    for(i=0;i<3;i++)
    {
        for(int j=0;j<4;j++)
        {
            printf("%-2d ",arr[i][j]);
        }
        printf("\n");
    }
    system("pause");
    return 0;
}
