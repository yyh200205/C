//不创建外部变量实现数据交换
int main()
{
	int a = 10;
	int b = 20;
	a = a ^ b;
	b = a ^ b;
	a = a ^ b;
	printf("a=%d b=%d\n", a, b);
	return 0;
}

int main()
{
	int a=10;
	int b=20;
	a=a+b;
	b=a-b;
	a=a-b;
	printf("a=%d b=%d\n",a,b);
	return 0;
}

//计算二进制中1的数量
int main()
{
	int num = 0;
	int count = 0;
	printf("请输入数值:");
	scanf_s("%d",&num);
	while (num)
	{
		if (num % 2 == 1)
		{
			count++;
		}
		num = num / 2;
	}
	printf("%d", count);
	return 0;
}
